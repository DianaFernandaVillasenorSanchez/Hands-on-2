(defrule enfermedades(enfermedad ?e) => (printout t ?e crlf))

CLIPS> (run)

depresion
Alzheimer
osteoporosis
hipertension
anemia
diabetes

(deftemplate signos-sintomas
  (slot enfermedad)
  (multislot signos)
  (multislot sintomas)
)

 (assert (signos-sintomas (enfermedad "diabetes") 
                          (signos "sed excesiva" "visión borrosa") 
                          (sintomas "aumento_orina" "fatiga")))

<Fact-8>
CLIPS> (assert (signos-sintomas (enfermedad "anemia") 
                          (signos "debilidad" "palidez") 
                          (sintomas "fatiga" "mareo")))
<Fact-9>
